{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "prc-platform/block-area",
	"version": "0.1.0",
	"title": "Block Area",
	"description": "A block area is like a template part but with greater editorial control.",
	"category": "theme",
	"keywords": [
		"block area",
		"block module",
		"topic lede",
		"featured lede"
	],
	"attributes": {
		"blockAreaSlug": {
			"type": "string"
		},
		"categorySlug": {
			"type": "string"
		},
		"inheritCategory": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"anchor": true,
		"html": false
	},
	"usesContext": [
		"postId",
		"query",
		"queryId",
		"templateSlug"
	],
	"textdomain": "prc-block-area",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
